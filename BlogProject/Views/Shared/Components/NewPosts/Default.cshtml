 @model List<Post>

    <div class="card">
                <div class="card-header">
                 <div class="card-title">Posts</div>
                </div>
                <div class="card-body">
                    <ul class="list-inline d-flex flex-wrap gap-2">
                        @foreach(var post in Model) {
                           <li class="list-inline-item">
                            <a href="/posts/@post.PostUrl" class="btn btn-outline-primary btn-sm">@post.PostContet</a>
                           </li>
                        }
                    </ul>
                </div>
            </div>